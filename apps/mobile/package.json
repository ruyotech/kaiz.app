{
  "name": "kaiz1-mobile-playground",
  "version": "1.0.0",
  "main": "expo-router/entry",
  "scripts": {
    "start": "expo start",
    "start:clear": "expo start --clear",
    "android": "expo run:android",
    "ios": "expo run:ios",
    "ios:clean": "rm -rf ios/build ~/Library/Developer/Xcode/DerivedData/KaizApp-* && expo run:ios",
    "ios:device": "expo run:ios --device",
    "prebuild": "expo prebuild",
    "prebuild:clean": "expo prebuild --clean",
    "web": "expo start --web",
    "clean": "rm -rf node_modules/.cache ios/build android/build ios/Pods ~/Library/Developer/Xcode/DerivedData/KaizApp-*",
    "clean:all": "rm -rf node_modules ios android && npm install && expo prebuild",
    "pods": "cd ios && pod install && cd ..",
    "pods:clean": "rm -rf ios/Pods ios/Podfile.lock && cd ios && pod install && cd .."
  },
  "dependencies": {
